<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="FossLock.Web" namespace="FossLock.Web.Model">

  <class name="Customer">
    <id name="Id">
      <generator class="int" />
    </id>
    <property name="Name" not-null="true" />
    <property name="Address1" />
    <property name="Address2" />
    <property name="City" />
    <property name="State" />
    <property name="PostalCode" />
    <property name="Phone1" />
    <property name="Phone2" />
    <property name="Fax" />
    <property name="Email" />
    <property name="ContactFirstName" />
    <property name="ContactLastName" />
    <property name="Notes" />
  </class>
</hibernate-mapping>